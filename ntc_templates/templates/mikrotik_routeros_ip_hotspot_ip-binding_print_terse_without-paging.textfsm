Value INDEX (\d+)
Value BY_PASSED (\S)
Value COMMENT (\S+)
Value MAC_ADDRESS (\S+)
Value ADDRESS (\S+)
Value TO_ADDRESS (\S+)
Value SERVER (\S+)
Value TYPE (\S+)

Start
  ^\s*${INDEX}\s+(?:${BY_PASSED}\s+)?(?:comment=${COMMENT}\s+)?(?:mac-address=${MAC_ADDRESS}\s+)?(?:address=${ADDRESS}\s+)?(?:to-address=${TO_ADDRESS}\s*)?(?:server=${SERVER}\s*)?(?:type=${TYPE}\s*)?\s*$$ -> Record
  ^. -> Error